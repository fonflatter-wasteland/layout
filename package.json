{
  "name": "fonflatter-layout",
  "version": "1.0.1-dev",
  "description": "layout for fonflatter.de",
  "private": true,
  "main": "index.js",
  "scripts": {
    "coverage": "coveralls < coverage/lcov.info",
    "pretest": "npm-check && jshint --extra-ext .js,._js . && jscs .",
    "test": "istanbul cover _mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fonflatter/layout"
  },
  "author": "winniehell <git@winniehell.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fonflatter/layout/issues"
  },
  "homepage": "https://github.com/fonflatter/layout",
  "dependencies": {
    "express": "^4.12.3",
    "nunjucks": "^1.3.3"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "jscs": "^1.12.0",
    "jshint": "^2.7.0",
    "lorem-ipsum": "^1.0.1",
    "mocha": "^2.2.1",
    "morgan": "^1.5",
    "npm-check": "^3.2.10",
    "supertest": "^0.15.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "app.js"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  }
}
